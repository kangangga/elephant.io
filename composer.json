{
    "name": "kangangga/elephant.io",
    "type": "library",
    "description": "Send events to a websocket",
    "license": "MIT",
    "homepage": "http://elephant.io",
    "require": {
        "php": ">=7.1.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "ElephantIO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElephantIO\\": "test/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}